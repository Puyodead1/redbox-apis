[ req ]
default_bits       = 2048
prompt             = no
default_md         = sha256
req_extensions     = req_ext
distinguished_name = dn

[ dn ]
CN = Athena.local
O  = Redbox APIs

[ req_ext ]
subjectAltName = @alt_names
extendedKeyUsage = serverAuth, clientAuth

[ alt_names ]
DNS.1 = Athena.local
IP.1  = 192.168.1.3
IP.2  = 127.0.0.1
